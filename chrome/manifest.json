{
  "name": "Acfun Fix",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Fix Acfun for old player",
  "default_locale": "en",
  "icons": {
    "128": "owl.png"
  },
  "background": {
    "scripts": ["background/index.js"]
  },
  "homepage_url": "https://www.cnbeining.com/",
  "permissions": ["webRequest", "http://cdn.aixifan.com/", "http:///www.acfun.tv/", "webRequestBlocking"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
